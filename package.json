{
  "name": "unifree-client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "lint:js:fix": "next lint --fix",
    "compile": "tsc --noEmit",
    "prepare": "husky install",
    "lint-staged": "lint-staged",
    "postinstall": "prisma generate",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "analyze": "ANALYZE=true next build",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:seed": "prisma db seed",
    "prisma:dev": "run-s -l prisma:generate prisma:migrate prisma:seed",
    "prisma:format": "npx prisma format",
    "prisma:studio": "npx prisma studio",
    "prisma:push": "npx prisma db push",
    "prisma:reset": "npx prisma db push --force-reset && npm run prisma:seed"
  },
  "dependencies": {
    "@floating-ui/dom": "^1.6.3",
    "@next/bundle-analyzer": "^14.1.0",
    "@prisma/client": "^5.8.1",
    "clsx": "^2.1.0",
    "date-fns": "^3.3.1",
    "envalid": "^8.0.0",
    "firebase": "^10.7.2",
    "firebase-admin": "^12.0.0",
    "next": "14.1.0",
    "next-auth": "^4.24.5",
    "prisma": "^5.9.1",
    "react": "^18",
    "react-dom": "^18",
    "server-only": "^0.0.1",
    "stripe": "^14.13.0",
    "swiper": "^11.0.5",
    "use-shopping-cart": "^3.2.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "@mermaid-js/mermaid-cli": "^10.8.0",
    "@storybook/addon-essentials": "^7.6.10",
    "@storybook/addon-interactions": "^7.6.10",
    "@storybook/addon-links": "^7.6.10",
    "@storybook/addon-onboarding": "^1.0.10",
    "@storybook/blocks": "^7.6.10",
    "@storybook/nextjs": "^7.6.10",
    "@storybook/react": "^7.6.10",
    "@storybook/test": "^7.6.10",
    "@tsconfig/strictest": "^2.0.2",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8",
    "eslint-config-next": "14.1.0",
    "eslint-plugin-storybook": "^0.6.15",
    "husky": "^8.0.0",
    "lint-staged": "^15.2.0",
    "prisma-erd-generator": "^1.11.2",
    "sass": "^1.70.0",
    "storybook": "^7.6.10",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5"
  },
  "volta": {
    "node": "18.17.0"
  },
  "prisma": {
    "seed": "ts-node -r tsconfig-paths/register --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  }
}
