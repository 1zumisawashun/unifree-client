{
  "name": "unifree-client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:prod": "npm run build && npm run start",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "lint:js": "eslint --ext .tsx,.ts app/",
    "lint:js:fix": "npm run lint:js -- --fix",
    "format": "prettier \"app/**/*.+(js|json|yml|ts|tsx)\"",
    "format:fix": "prettier --write \"app/**/*.+(js|json|yml|ts|tsx)\"",
    "compile": "tsc --noEmit",
    "compile:trace": "tsc --generateTrace typescript --incremental false",
    "prepare": "husky install",
    "lint-staged": "lint-staged",
    "postinstall": "prisma generate",
    "vercel-build": "prisma generate && next build && prisma migrate deploy",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "analyze": "ANALYZE=true next build",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:seed": "prisma db seed",
    "prisma:dev": "run-s -l prisma:generate prisma:migrate prisma:seed",
    "prisma:format": "npx prisma format",
    "prisma:studio": "npx prisma studio",
    "prisma:push": "npx prisma db push",
    "prisma:reset": "npx prisma db push --force-reset && npm run prisma:seed",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --ci",
    "coverage:ci": "jest --coverage --silent --ci --testLocationInResults --json --outputFile=\"report.json\"",
    "hcm": "hcm 'app/**/*.module.{css,scss,less}' --webpackResolveAlias='{\"@\": \"app\"}'",
    "hcm:watch": "npm run hcm -- --watch",
    "hcm:remove": "ts-node scripts/remove-hcm.ts"
  },
  "dependencies": {
    "@floating-ui/dom": "^1.6.3",
    "@hookform/resolvers": "^3.3.4",
    "@next/bundle-analyzer": "^14.1.0",
    "@prisma/client": "^5.8.1",
    "@vercel/analytics": "^1.2.2",
    "@vercel/speed-insights": "^1.0.10",
    "clsx": "^2.1.0",
    "date-fns": "^3.3.1",
    "envalid": "^8.0.0",
    "firebase": "^10.7.2",
    "firebase-admin": "^12.0.0",
    "next": "14.1.0",
    "next-auth": "^4.24.5",
    "prisma": "^5.9.1",
    "react": "^18",
    "react-dom": "^18",
    "react-hook-form": "^7.50.1",
    "server-only": "^0.0.1",
    "stripe": "^14.13.0",
    "swiper": "^11.0.5",
    "use-shopping-cart": "^3.2.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^1.3.3",
    "@faker-js/faker": "^8.4.1",
    "@mermaid-js/mermaid-cli": "^10.8.0",
    "@storybook/addon-essentials": "^8.0.8",
    "@storybook/addon-interactions": "^8.0.8",
    "@storybook/addon-links": "^8.0.8",
    "@storybook/addon-onboarding": "^8.0.8",
    "@storybook/blocks": "^8.0.8",
    "@storybook/nextjs": "^8.0.8",
    "@storybook/react": "^8.0.8",
    "@storybook/test": "^8.0.8",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.2",
    "@tsconfig/strictest": "^2.0.2",
    "@types/jest": "^29.5.12",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8",
    "eslint-config-next": "14.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-storybook": "^0.6.15",
    "eslint-plugin-testing-library": "^6.2.0",
    "happy-css-modules": "^2.1.2",
    "husky": "^8.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.2.0",
    "msw": "^2.2.13",
    "next-router-mock": "^0.9.13",
    "prettier": "^3.2.5",
    "prisma-erd-generator": "^1.11.2",
    "sass": "^1.70.0",
    "storybook": "^8.0.8",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5"
  },
  "volta": {
    "node": "18.17.0",
    "npm": "9.6.7"
  },
  "prisma": {
    "seed": "ts-node -r tsconfig-paths/register --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  },
  "nextBundleAnalysis": {
    "budget": 358400,
    "budgetPercentIncreaseRed": 20,
    "minimumChangeThreshold": 0,
    "showDetails": true
  }
}